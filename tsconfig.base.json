{
  "include": [],
  "compilerOptions": {
    "strict": true,
    "moduleDetection": "force",
    "composite": true,
    "downlevelIteration": true,
    "resolveJsonModule": true,
    "esModuleInterop": false,
    "declaration": true,
    "skipLibCheck": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "NodeNext",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "isolatedModules": true,
    "sourceMap": true,
    "declarationMap": true,
    "noImplicitReturns": false,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "noFallthroughCasesInSwitch": true,
    "noEmitOnError": false,
    "noErrorTruncation": true,
    "allowJs": false,
    "checkJs": false,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUncheckedIndexedAccess": false,
    "strictNullChecks": true,
    "baseUrl": ".",
    "target": "ES2022",
    "module": "NodeNext",
    "incremental": true,
    "removeComments": false,
    "paths": {
      "@effect/platform/test/*": ["./packages/platform/test/*.js"],
      "@effect/platform/*": ["./packages/platform/src/*.js"],
      "@effect/platform-bun/test/*": ["./packages/platform-bun/test/*.js"],
      "@effect/platform-bun/*": ["./packages/platform-bun/src/*.js"],
      "@effect/platform-browser/test/*": [
        "./packages/platform-browser/test/*.js"
      ],
      "@effect/platform-browser/*": ["./packages/platform-browser/src/*.js"],
      "@effect/platform-node/test/*": ["./packages/platform-node/test/*.js"],
      "@effect/platform-node/*": ["./packages/platform-node/src/*.js"]
    },
    "plugins": [
      {
        "name": "@effect/language-service"
      }
    ]
  }
}
