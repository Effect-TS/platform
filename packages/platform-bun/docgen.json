{
  "exclude": ["src/internal/**/*.ts"],
  "theme": "mikearnaldi/just-the-docs",
  "parseCompilerOptions": {
    "noEmit": true,
    "strict": true,
    "target": "es2015",
    "lib": ["es2015"],
    "paths": {
      "@effect/platform": ["../platform/src/index.ts"],
      "@effect/platform/*": ["../platform/src/*"],
      "@effect/platform-bun": ["./src/index.ts"],
      "@effect/platform-bun/*": ["./src/*"],
      "@effect/platform-node": ["../platform-node/src/index.ts"],
      "@effect/platform-node/*": ["../platform-node/src/*"]
    }
  },
  "examplesCompilerOptions": {
    "noEmit": true,
    "strict": true,
    "target": "es2015",
    "lib": ["es2015"],
    "paths": {
      "@effect/platform": ["../platform/src/index.ts"],
      "@effect/platform/*": ["../platform/src/*"],
      "@effect/platform-bun": ["./src/index.ts"],
      "@effect/platform-bun/*": ["./src/*"],
      "@effect/platform-node": ["../platform-node/src/index.ts"],
      "@effect/platform-node/*": ["../platform-node/src/*"]
    }
  }
}
