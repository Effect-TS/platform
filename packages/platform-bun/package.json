    "src",
    "dist",
    "BunContext",
    "Command",
    "CommandExecutor",
    "Effectify",
    "Error",
    "FileSystem",
    "Http",
    "HttpClient",
    "HttpServer",
    "KeyValueStore",
    "Path",
    "Runtime",
    "Sink",
    "Stream"
  ],
