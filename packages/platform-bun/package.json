{
  "name": "@effect/platform-bun",
  "version": "0.8.1",
  "description": "Unified interfaces for common platform-specific services",
  "main": "dist/effect-platform-bun.cjs.js",
  "module": "dist/effect-platform-bun.esm.js",
  "preconstruct": {
    "entrypoints": [
      "*.ts",
      "Http/*.ts"
    ],
    "distFilenameStrategy": "full",
    "exports": {
      "importConditionDefaultExport": "default"
    }
  },
  "effect": {
    "generateIndex": true
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/effect-ts/platform.git"
  },
  "homepage": "https://github.com/effect-ts/platform",
  "scripts": {
    "example": "ts-node --project tsconfig.examples.json",
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "tc": "tsc --noEmit",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "keywords": [],
  "author": "Effect contributors",
  "license": "MIT",
  "sideEffects": false,
  "dependencies": {
    "@effect/platform": "workspace:^",
    "@effect/platform-node": "workspace:^"
  },
  "devDependencies": {
    "@effect/schema": "^0.40.0",
    "bun-types": "^1.0.3",
    "effect": "2.0.0-next.45"
  },
  "peerDependencies": {
    "effect": "^2.0.0-next.45"
  },
