{
  "name": "@effect/platform-browser",
  "version": "0.14.7",
  "description": "Unified interfaces for common platform-specific services",
  "main": "dist/effect-platform-browser.cjs.js",
  "module": "dist/effect-platform-browser.esm.js",
  "preconstruct": {
    "entrypoints": [
      "*.ts"
    ],
    "distFilenameStrategy": "full",
    "exports": {
      "importConditionDefaultExport": "default"
    }
  },
  "effect": {
    "generateIndex": true
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/effect-ts/platform.git"
  },
  "homepage": "https://github.com/effect-ts/platform",
  "scripts": {
    "example": "ts-node --project tsconfig.examples.json",
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "tc": "tsc --noEmit",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "keywords": [],
  "author": "Effect contributors",
  "license": "MIT",
  "sideEffects": false,
  "dependencies": {
    "@effect/platform": "workspace:^"
  },
  "devDependencies": {
    "effect": "2.0.0-next.52",
    "happy-dom": "^12.9.1",
    "vitest-localstorage-mock": "^0.0.1"
  },
  "peerDependencies": {
    "effect": "2.0.0-next.52"
  },
  "files": [
    "src",
    "dist",
    "Clipboard",
    "Effectify",
    "Error",
    "HttpClient",
    "KeyValueStore",
    "Path",
    "Runtime",
    "Worker",
    "WorkerRunner"
  ],
  "exports": {
    "./Path": {
      "module": "./Path/dist/effect-platform-browser-Path.esm.js",
      "import": "./Path/dist/effect-platform-browser-Path.cjs.mjs",
      "default": "./Path/dist/effect-platform-browser-Path.cjs.js"
    },
    "./Error": {
      "module": "./Error/dist/effect-platform-browser-Error.esm.js",
      "import": "./Error/dist/effect-platform-browser-Error.cjs.mjs",
      "default": "./Error/dist/effect-platform-browser-Error.cjs.js"
    },
    ".": {
      "module": "./dist/effect-platform-browser.esm.js",
      "import": "./dist/effect-platform-browser.cjs.mjs",
      "default": "./dist/effect-platform-browser.cjs.js"
    },
    "./Worker": {
      "module": "./Worker/dist/effect-platform-browser-Worker.esm.js",
      "import": "./Worker/dist/effect-platform-browser-Worker.cjs.mjs",
      "default": "./Worker/dist/effect-platform-browser-Worker.cjs.js"
    },
    "./Runtime": {
      "module": "./Runtime/dist/effect-platform-browser-Runtime.esm.js",
      "import": "./Runtime/dist/effect-platform-browser-Runtime.cjs.mjs",
      "default": "./Runtime/dist/effect-platform-browser-Runtime.cjs.js"
    },
    "./Clipboard": {
      "module": "./Clipboard/dist/effect-platform-browser-Clipboard.esm.js",
      "import": "./Clipboard/dist/effect-platform-browser-Clipboard.cjs.mjs",
      "default": "./Clipboard/dist/effect-platform-browser-Clipboard.cjs.js"
    },
    "./Effectify": {
      "module": "./Effectify/dist/effect-platform-browser-Effectify.esm.js",
      "import": "./Effectify/dist/effect-platform-browser-Effectify.cjs.mjs",
      "default": "./Effectify/dist/effect-platform-browser-Effectify.cjs.js"
    },
    "./HttpClient": {
      "module": "./HttpClient/dist/effect-platform-browser-HttpClient.esm.js",
      "import": "./HttpClient/dist/effect-platform-browser-HttpClient.cjs.mjs",
      "default": "./HttpClient/dist/effect-platform-browser-HttpClient.cjs.js"
    },
    "./WorkerRunner": {
      "module": "./WorkerRunner/dist/effect-platform-browser-WorkerRunner.esm.js",
      "import": "./WorkerRunner/dist/effect-platform-browser-WorkerRunner.cjs.mjs",
      "default": "./WorkerRunner/dist/effect-platform-browser-WorkerRunner.cjs.js"
    },
    "./KeyValueStore": {
      "module": "./KeyValueStore/dist/effect-platform-browser-KeyValueStore.esm.js",
      "import": "./KeyValueStore/dist/effect-platform-browser-KeyValueStore.cjs.mjs",
      "default": "./KeyValueStore/dist/effect-platform-browser-KeyValueStore.cjs.js"
    },
    "./package.json": "./package.json"
  }
}
